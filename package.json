{
  "name": "rn-debug-overlay",
  "version": "0.2.0",
  "description": "In-app debug overlay for React Native (iOS, Android, Web). Logs • Network • Perf • Device • Export — zero native modules.",
  "license": "MIT",
  "author": "Anurag Singh",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anu-Code07/rn-debug-overlay.git"
  },
  "bugs": {
    "url": "https://github.com/Anu-Code07/rn-debug-overlay/issues"
  },
  "homepage": "https://github.com/Anu-Code07/rn-debug-overlay#readme",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "react-native": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.73.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "optionalPeerDependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-native": "^0.72.8",
    "typescript": "^5.4.0",
    "vite": "^5.3.0",
    "vite-plugin-dts": "^4.2.1"
  },
  "keywords": [
    "react-native",
    "debug",
    "overlay",
    "network",
    "logs",
    "performance",
    "react-native-web",
    "vite",
    "library"
  ]
}
